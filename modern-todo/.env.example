VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Auth
VITE_REQUIRE_LOGIN=true

# Supabase (CRM)
VITE_CRM_WORKSPACE=default
VITE_CRM_KV_TABLE=crm_kv

# Opcional: habilita provedor "Teste (local)" no Agente IA (apenas para dev)
VITE_ENABLE_DEMO_PROVIDER=false

# CRM API (Evolution webhook/proxy)
CRM_API_PORT=8787

# Se definido, o webhook exige ?token=... ou header x-webhook-token com este valor
EVOLUTION_WEBHOOK_TOKEN=coloque-um-token-seguro-aqui
